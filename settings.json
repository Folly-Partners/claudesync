{
  "enabledPlugins" : {
    "compound-engineering@every-marketplace" : true,
    "firebase@claude-plugins-official" : true,
    "github@claude-plugins-official" : true,
    "security-guidance@claude-plugins-official" : true,
    "stripe@claude-plugins-official" : true,
    "supabase@claude-plugins-official" : true,
    "vercel@claude-plugins-official" : true
  },
  "hooks" : {
    "Notification" : [
      {
        "hooks" : [
          {
            "command" : "python3 ~\/.claude\/hooks\/claude-island-state.py",
            "type" : "command"
          }
        ],
        "matcher" : "*"
      }
    ],
    "PermissionRequest" : [
      {
        "hooks" : [
          {
            "command" : "python3 ~\/.claude\/hooks\/claude-island-state.py",
            "timeout" : 86400,
            "type" : "command"
          }
        ],
        "matcher" : "*"
      }
    ],
    "PostToolUse" : [
      {
        "hooks" : [
          {
            "command" : "python3 ~\/.claude\/hooks\/claude-island-state.py",
            "type" : "command"
          }
        ],
        "matcher" : "*"
      }
    ],
    "PreCompact" : [
      {
        "hooks" : [
          {
            "command" : "python3 ~\/.claude\/hooks\/claude-island-state.py",
            "type" : "command"
          }
        ],
        "matcher" : "auto"
      },
      {
        "hooks" : [
          {
            "command" : "python3 ~\/.claude\/hooks\/claude-island-state.py",
            "type" : "command"
          }
        ],
        "matcher" : "manual"
      }
    ],
    "PreToolUse" : [
      {
        "hooks" : [
          {
            "command" : "python3 ~\/.claude\/hooks\/claude-island-state.py",
            "type" : "command"
          }
        ],
        "matcher" : "*"
      }
    ],
    "SessionEnd" : [
      {
        "hooks" : [
          {
            "command" : "python3 ~\/.claude\/hooks\/claude-island-state.py",
            "type" : "command"
          }
        ]
      }
    ],
    "SessionStart" : [
      {
        "hooks" : [
          {
            "command" : "python3 ~\/.claude\/hooks\/claude-island-state.py",
            "type" : "command"
          }
        ]
      }
    ],
    "Stop" : [
      {
        "hooks" : [
          {
            "command" : "python3 ~\/.claude\/hooks\/claude-island-state.py",
            "type" : "command"
          }
        ]
      }
    ],
    "SubagentStop" : [
      {
        "hooks" : [
          {
            "command" : "python3 ~\/.claude\/hooks\/claude-island-state.py",
            "type" : "command"
          }
        ]
      }
    ],
    "UserPromptSubmit" : [
      {
        "hooks" : [
          {
            "command" : "python3 ~\/.claude\/hooks\/claude-island-state.py",
            "type" : "command"
          }
        ]
      }
    ]
  },
  "model" : "opus",
  "permissions" : {
    "allow" : [
      "Bash",
      "Read",
      "Write",
      "Edit",
      "MultiEdit",
      "NotebookEdit",
      "WebFetch",
      "WebSearch",
      "Glob",
      "Grep",
      "Task",
      "TodoWrite",
      "mcp__tavily",
      "mcp__SuperThings",
      "mcp__browserbase",
      "mcp__zapier",
      "mcp__ahrefs",
      "mcp__xero",
      "mcp__hunter"
    ],
    "deny" : [
      "Bash(sudo:*)",
      "Bash(sudo :*)",
      "Bash(su :*)",
      "Bash(su -:*)",
      "Bash(doas:*)",
      "Bash(rm -rf \/:*)",
      "Bash(rm -rf ~:*)",
      "Bash(rm -rf \/*:*)",
      "Bash(rm -rf $HOME:*)",
      "Bash(rm -rf --no-preserve-root:*)",
      "Bash(mkfs:*)",
      "Bash(dd if=\/dev:*)",
      "Bash(dd of=\/dev:*)",
      "Bash(fdisk:*)",
      "Bash(parted:*)",
      "Bash(shred:*)",
      "Bash(wipefs:*)",
      "Bash(chmod -R 777 \/:*)",
      "Bash(chmod 777 \/:*)",
      "Bash(chown -R :*)",
      "Bash(> \/dev\/sda:*)",
      "Bash(> \/dev\/disk:*)",
      "Bash(mv ~ :*)",
      "Bash(mv \/ :*)",
      "Bash(mv ~\/ :*)",
      "Bash(git push --force origin main:*)",
      "Bash(git push --force origin master:*)",
      "Bash(git push -f origin main:*)",
      "Bash(git push -f origin master:*)"
    ]
  }
}