<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.andrews-plugin.sync</string>
    <key>ProgramArguments</key>
    <array>
        <string>__PLUGIN_DIR__/scripts/sync.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>9</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>
    <key>StandardOutPath</key>
    <string>__PLUGIN_DIR__/sync.log</string>
    <key>StandardErrorPath</key>
    <string>__PLUGIN_DIR__/sync.log</string>
</dict>
</plist>
